<http://semantic-machines.com/veda/veda-schema-EmployeeProfile>
  rdf:type owl:Ontology ;
  rdfs:label "Онтология Веда. Досье сотрудника. Схема."@ru ;
  rdfs:label "Veda ontology. EmployeeProfile. Schema."@en ;
  v-s:loadPriority 20 ;
.

# ------------------------------------------------------------ КЛАССЫ --

###----------------------------------Досье сотрудника---------------------
v-s:EmployeeProfile
  rdf:type owl:Class ;
  rdfs:subClassOf v-s:UserThing ;
  rdfs:subClassOf v-s:Labeled ;
  rdfs:subClassOf v-s:BackwardLink ;
  rdfs:label "Досье сотрудника"@ru ;
  rdfs:label "Employee profile"@en ;
  v-s:labelPattern "{@.v-s:creator}" ;
.
v-s:hasProfession
  rdf:type owl:ObjectProperty ;
  rdfs:label "Вид работ сотрудника"@ru ;
  rdfs:label "Kind of work for Employee"@en ;
  rdfs:range v-s:Profession ;
  rdfs:domain v-s:EmployeeProfile ;
.
# ------------------------------------------------------------
v-s:Profession
  rdf:type owl:Class ;
  rdfs:label "Профессия"@ru ;
  rdfs:label "Profession list"@en ;
  v-ui:hasTemplate v-ui:ClassNameLabelLinkTemplate ;
.
