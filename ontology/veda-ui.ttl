v-ui:IconModalTemplate
  rdf:type v-ui:ClassTemplate ;
  rdfs:label "Шаблон с меткой и ссылкой"@ru ;
  rdfs:label "Template with label & link"@en ;
  v-ui:template """
<a href="#" class="glyphicon glyphicon-zoom-in"></a>
<script>
  template.on("click", function (e) {
    e.preventDefault();
    var newMode = "view";
    var parentTmpl = template.parent().closest("[resource]");
    if (parentTmpl.attr("resource") === individual.id) {
      newMode = parentTmpl.data("mode");
    }
    var modal;
    if(newMode==="view") {
      if (!individual.rights.hasValue("v-s:canUpdate", true)) {
        modal = $("#individual-modal-template").html();
      }
      else if(individual.rights.hasValue("v-s:canUpdate", true)) {
        modal = $("#notification-modal-template").html();
      }
    } else if (newMode==="edit") {
      modal = $("#individual-modal-template").html();
    }
    modal = $(modal).modal({"show": false});
    $("body").append(modal);
    modal.modal("show");
    template.one("remove", function () {
      modal.modal("hide").remove();
    });
    var cntr = $(".modal-body", modal);
    var ok = $("#ok", modal);
    cntr.on("valid", function () {
      ok.removeAttr("disabled");
    });
    cntr.on("invalid", function () {
    ok.attr("disabled", "disabled");
    });
 // var tmpl = individual["rdf:type"][0].template ? $( individual["rdf:type"][0].template["v-ui:template"][0].toString() ) : undefined;
    var tmpl = individual["rdf:type"][0].hasValue("v-ui:hasTemplate") ? $( individual["rdf:type"][0]["v-ui:hasTemplate"][0]["v-ui:template"][0].toString() ) : undefined;

    if (newMode === "edit") {
      individual.present(cntr, tmpl, newMode);
      $(".actions", tmpl).remove();
    } else {
      $("#cancel", tmpl).addClass("viev").removeClass("-view");
      individual.present(cntr, tmpl, newMode);
    }
    individual.one("beforeReset", function () {
      modal.modal("hide").remove();
    });
    individual.one("afterSave", function () {
      modal.modal("hide").remove();
    });
  });
  //# sourceURL=v-ui:IconModalTemplate
</script>
  """ ;
.

