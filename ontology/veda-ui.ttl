v-ui:VersionTemplate
  rdf:type v-ui:ClassTemplate ;
  rdfs:label "Шаблон для класса v-s:Version"@ru ;
  rdfs:label "Template for class v-s:Version"@en ;
  v-ui:forClass v-s:Version ;
  v-ui:template "" ;
